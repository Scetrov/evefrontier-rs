{
  "name": "evefrontier-service-route",
  "projectType": "application",
  "sourceRoot": "crates/evefrontier-service-route/src",
  "tags": ["scope:service", "type:app"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo build -p evefrontier-service-route",
        "cwd": "{workspaceRoot}"
      },
      "outputs": ["{workspaceRoot}/target/debug/evefrontier-service-route"]
    },
    "test": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "cargo test -p evefrontier-service-route",
        "cwd": "{workspaceRoot}"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo fmt -p evefrontier-service-route -- --check",
        "cwd": "{workspaceRoot}"
      }
    },
    "clippy": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "cargo clippy -p evefrontier-service-route --all-targets --all-features -- -D warnings",
        "cwd": "{workspaceRoot}"
      }
    },
    "complexity": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "cargo clippy -p evefrontier-service-route --all-targets -- -W clippy::cognitive_complexity -W clippy::too_many_lines",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}
